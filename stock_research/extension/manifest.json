{
    "manifest_version": 3,
    "name": "Stock Research Assistant",
    "version": "1.0",
    "description": "AI-powered stock research assistant",
    "permissions": [
      "sidePanel",
      "activeTab"
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }